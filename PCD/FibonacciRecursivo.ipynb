   "source": [
    "def fibo_recursivo(n):\n",
    "    if n <= 1:\n",
    "        return n #Caso base\n",
    "    else:\n",
    "        return fibo_recursivo(n - 1) + fibo_recursivo(n - 2)\n",
    "#LLamadas recursivas\n",
    "[fibo_recursivo(x) for x in range(10)]"
   ]
